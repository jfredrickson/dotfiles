
include "%L"   # import the default Compose file for your locale

<Multi_key> <question> <question>       : "⸮"   # U2E2E   # sarcasm sign
<Multi_key> <slash> <slash>             : "ϟ"   # U03DF   # ACϟDC
<Multi_key> <6> <6> <6>                 : "🤘"  #         # Sign of the Horns
<Multi_key> <f> <p>                     : "🤦"  # U1F926  # Facepalm
<Multi_key> <m> <m>                     : "♫"   # U266B   # music sign
<Multi_key> <y> <n>                     : "👌"  # 1F44C   # OK hand sign
<Multi_key> <y> <y>                     : "👍"  # 1F44D   # Thumbs up
<Multi_key> <n> <n>                     : "👎"  # 1F44E   # Thumbs down
<Multi_key> <s> <h>                     : "🤷"  # 1F44E   # shrug ¯\_(ツ)_/¯

