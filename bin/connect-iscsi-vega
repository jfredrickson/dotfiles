#!/bin/sh

#sudo iscsiadm -m discovery --type sendtargets  -p files.lan.vanginderachter.be

case $1 in
    "start")
        sudo iscsiadm -m node --target iqn.2000-01.com.synology:vega.ymedia.6fbb064b96 --login
        ;;
    "stop")
        sudo iscsiadm -m node --target iqn.2000-01.com.synology:vega.ymedia.6fbb064b96 --logout
        ;;
    *)
        echo start or stop
        ;;
esac

